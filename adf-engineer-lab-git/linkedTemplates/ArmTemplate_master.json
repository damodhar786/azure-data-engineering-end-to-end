{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-engineer-lab-git"
		},
		"ls_adls_fmcg_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_fmcg'"
		},
		"ls_sql_ingestion_metadata_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sql_ingestion_metadata'"
		},
		"ls_adls_fmcg_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsdeprod01.dfs.core.windows.net/"
		},
		"ls_sql_ingestion_metadata_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-metadata-lab.database.windows.net"
		},
		"ls_sql_ingestion_metadata_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldb_ingestion_metadata"
		},
		"ls_sql_ingestion_metadata_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "data_engineer_sql"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-engineer-lab-git_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_fmcg_accountKey": {
						"value": "[parameters('ls_adls_fmcg_accountKey')]"
					},
					"ls_sql_ingestion_metadata_password": {
						"value": "[parameters('ls_sql_ingestion_metadata_password')]"
					},
					"ls_adls_fmcg_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_fmcg_properties_typeProperties_url')]"
					},
					"ls_sql_ingestion_metadata_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_ingestion_metadata_properties_typeProperties_server')]"
					},
					"ls_sql_ingestion_metadata_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_ingestion_metadata_properties_typeProperties_database')]"
					},
					"ls_sql_ingestion_metadata_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_ingestion_metadata_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}